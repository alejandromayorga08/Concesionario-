abstract class Auto {
    protected String marca;
    protected String modelo;
    protected int a単o;
    protected double precioBase;

    public Auto() {
    }

    public Auto(String marca, String modelo, int a単o, double precioBase) {
        this.marca = marca;
        this.modelo = modelo;
        this.a単o = a単o;
        this.precioBase = precioBase;
    }

    public double calularPrecioFinal(){
         return precioBase * 1.10;
    }

    public abstract String getDetalles();



}
