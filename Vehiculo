public class Vehiculo extends Auto {
    private int numPuertas;

    public Vehiculo(int numPuertas,String marca,String modelo,int a単o,double precioBase) {
        super(marca,modelo,a単o,precioBase);
        this.numPuertas = numPuertas;
    }

    @Override
    public String getDetalles() {
        return "Auto- Marca: "+marca+", Modelo: "+modelo+", A単o: "+a単o+", Precio base: $"+precioBase+", Numero de puertas: "+numPuertas+", Precio final: "+calularPrecioFinal();
    }
}
