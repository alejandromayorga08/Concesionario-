public class Motocicleta extends Auto {
    private int cilindrada;

    public Motocicleta(String marca, String modelo, int a単o, double precioBase, int cilindrada) {
        super(marca, modelo, a単o, precioBase);
        this.cilindrada = cilindrada;
    }

    @Override
    public String getDetalles() {
        return "Motocicleta- Marca: "+marca+", Modelo: "+modelo+", A単o: "+a単o+", Precio base:  "+precioBase+", Cilindrada: "+cilindrada+", Precio final: "+calularPrecioFinal();
    }
}
